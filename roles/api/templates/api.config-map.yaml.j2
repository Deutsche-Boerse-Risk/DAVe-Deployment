apiVersion: v1
kind: ConfigMap
metadata:
  name: dave-api
  namespace: {{ namespace }}
  labels:
    app: dave
    layer: api
    project: dave
data:
  log.level: "INFO"
  db.name: "{{ database_name }}"
  db.connection_url: "{{ database_connection_url }}"
  http.enable: "TRUE"
  http.compression: "TRUE"
  http.auth: "TRUE"
  http.cors: "TRUE"
  http.cors_origin: "*"
