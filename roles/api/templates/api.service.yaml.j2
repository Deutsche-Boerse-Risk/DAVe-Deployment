apiVersion: v1
kind: Service
metadata:
  labels:
    app: dave
    layer: api
    project: dave
  name: dave-api
  namespace: {{ namespace }}
spec:
  ports:
    - name: https
      port: 443
      targetPort: 8181
    - name: http
      port: 80
      targetPort: 8080
  selector:
    name: dave-api
  type: LoadBalancer
